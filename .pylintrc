[MESSAGES CONTROL]

# I0011: Locally disabling %s
# W0142: Used * or ** magic
# W0511: TODO in comments
disable=I0011,W0142,W0511


[REPORTS]

output-format=colorized

reports=no

msg-template={msg_id}:{line:3d},{column}:{msg}


[FORMAT]

max-line-length=79

ignore-long-lines=^(\s*(# )?(<?https?://\S+>?))|(.* pragma: \S+.*)$
