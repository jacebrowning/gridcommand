{
    "python.formatting.provider": "black",
    "python.linting.mypyEnabled": true,
    "editor.formatOnSave": true,
    "cSpell.words": [
        "ABCDEFGHJKLMNPQRTUVXYZ",
        "autoplay",
        "livereload",
        "showfspath",
        "dataclass"
    ],
    "files.exclude": {
        "**/__pycache__/": true,
        ".cache/": true,
        ".venv/": true
    }
}
